{
  "name": "ApprisED",
  "version": "0.1.0",
  "description": "Educational communication app using ejs, tailwindcss, and JS",
  "main": "server.js",
  "scripts": {
    "build": "tailwindcss -i ./public/css/input.css -o ./public/css/main.css",
    "watch": "tailwindcss -i ./public/css/input.css -o ./public/css/main.css --watch",
    "start": "node server.js",
    "dev": "set \"NODE_ENV=development\" && nodemon server.js"
  },
  "author": "Jeff Whetzal",
  "license": "ISC",
  "dependencies": {
    "connect-mongo": "^4.6.0",
    "daisyui": "^2.31.0",
    "dotenv": "^16.0.2",
    "express-session": "^1.17.3",
    "mongoose": "^6.6.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.19",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "tailwindcss": "^3.1.8"
  }
}
